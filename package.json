{
  "name": "celebrity-bucket-mobile",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://fisnikmorina.github.io/celebrity-game/",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export --platform web --public-url /celebrity-game/",
    "deploy": "npm run build:web && cp dist/index.html dist/404.html && npx gh-pages -d dist"
  },
  "dependencies": {
    "expo": "~54.0.29",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2"
  }
}
